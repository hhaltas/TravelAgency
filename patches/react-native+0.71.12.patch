+{
+  static dispatch_once_t onceToken;
+  dispatch_once(&onceToken, ^{
+    networkThread = [_RCTSRRunLoopThread new];
+    networkThread.name = @"com.squareup.SocketRocket.NetworkThread";
+    [networkThread start];
+    networkRunLoop = networkThread.runLoop;
+  });
